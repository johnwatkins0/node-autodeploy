{
  "name": "@johnwatkins0/node-autodeploy",
  "version": "1.0.23",
  "description": "Deploy a project to a remote server when it's pushed to GitHub. ",
  "bin": {
    "autodeploy": "."
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepare": "npm run build",
    "start": "babel src --out-dir dist --watch",
    "lint": "eslint src/",
    "test": "yarn lint && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatkins0/node-autodeploy.git"
  },
  "author": "John Watkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatkins0/node-autodeploy/issues"
  },
  "homepage": "https://github.com/johnwatkins0/node-autodeploy#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^1.0.0-rc.2",
    "jest": "^22.4.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  }
}
